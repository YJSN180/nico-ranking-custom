{
  "name": "nico-ranking-new",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "npx @cloudflare/next-on-pages@1 --experimental-minify",
    "build:cloudflare": "next build",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:all": "npm run test -- --run && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "check:all": "npm run typecheck && npm run lint && npm run test -- --run && npm run build",
    "check:convex": "npx tsx scripts/check-convex-health.ts",
    "check:kv": "npx tsx scripts/check-kv-freshness.ts",
    "test:production": "npx tsx scripts/test-production.ts",
    "verify:deployment": "npx tsx scripts/verify-cloudflare-deployment.ts",
    "deploy:check": "npm run check:convex && npm run check:kv",
    "deploy:verify": "npm run verify:deployment"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "cheerio": "^1.0.0",
    "convex": "^1.24.8",
    "fast-xml-parser": "^4.5.3",
    "next": "14.2.21",
    "node-fetch": "^2.7.0",
    "pako": "^2.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@playwright/test": "^1.49.1",
    "@stagewise/toolbar-next": "^0.1.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pako": "^2.0.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.21",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "puppeteer": "^24.9.0",
    "typescript": "^5",
    "vercel": "^42.3.0",
    "vitest": "^2.1.8"
  }
}
